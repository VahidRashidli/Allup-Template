@model FooterIndexViewModel
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title"> Footer </h4>
                <div class="table-responsive pt-3">
                    <table class="table table-dark">
                        <thead>
                            <tr>                              
                                <th>
                                    Footer Links
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr><td>@Model.Footer.FacebookLink</td></tr>
                            <tr><td>@Model.Footer.LinkedInLink</td></tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Footer Categories </h4>
                <div class="table-responsive pt-3">
                    <table class="table table-dark">
                        <thead>
                            <tr>
                                <th>
                                    Footer Category Id
                                </th>
                                <th>
                                    Footer Category Name
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (FooterCategory footerCategory in Model.FooterCategories)
                            {

                                <tr>
                            <td>@footerCategory.Id</td>
                            <td>@footerCategory.Name</td>
                                </tr>
                                
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Footer Categories </h4>
                <div class="table-responsive pt-3">
                    <table class="table table-dark">
                        <thead>
                            <tr>
                                <th>
                                    Footer Category Section Id
                                </th>
                                <th>
                                    Footer Category Section Name
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (FooterCategorySection footerCategorySection in Model.FooterCategorySections)
                            {

                                <tr>
                            <td>@footerCategorySection.Id</td>
                            <td>@footerCategorySection.Name</td>
                                </tr>
                                
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>